{
  "name": "lenfi-sdk",
  "version": "1.0.0",
  "description": "SDK for LenFi Finance",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm depositScript.ts",
    "test:deposit": "node --experimental-specifier-resolution=node --loader ts-node/esm depositScript.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bignumber.js": "^9.1.2",
    "dotenv": "^16.4.5",
    "lucid-cardano": "^0.10.9"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^14.14.31",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.2.2"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  }
}
